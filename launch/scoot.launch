<launch>
  <group ns="$(arg name)">
    <include file="$(find obstacle)/launch/obstacle.xml">
      <arg name="name" value="$(arg name)"/>
    </include>
    <include file="$(find scoot)/launch/scoot.xml">
      <arg name="name" value="$(arg name)"/>
      <arg name="name_tf" value="$(arg name_tf)"/>
    </include>
  </group>
  
</launch>
